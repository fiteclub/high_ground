def cardinal_direction(x, y)
    cardinal = Array.new
    if thor_x < light_x
        thor_x += 1
        cardinal << 'N'
    elsif thor_x > light_x
        thor_x -= 1
        cardinal << 'S'
    else
    end
    if thor_y < light_y
        thor_y += 1
        cardinal << 'E'
    elsif thor_y < light_y
        thor_y -= 1
        cardinal << 'W'
    else
    end
    cardinal.join('')
end
